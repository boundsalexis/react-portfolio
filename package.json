{
  "name": "react-portfolio",
  "version": "1.0.0",
  "description": "react portfolio",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boundsalexis/react-portfolio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boundsalexis/react-portfolio/issues"
  },
  "homepage": "https://github.com/boundsalexis/react-portfolio#readme",
  "dependencies": {
   
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
  }
}
